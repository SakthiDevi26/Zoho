	printmenu.printSavingsUserMenu();
	int choice = scanner.nextInt();
	switch(choice)
	{
	case 1:
		//Deposit money
	case 2:
		//withdraw
	case 3:
		//swipe
	case 4:
		//apply
	case 5:
		//view
	case 6:
		//logout
	default:
		System.out.println("Enter valid choice");
	}
	}
}
