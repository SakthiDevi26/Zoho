package Admin;

import basics.GetDetails;

public class AdminLogin {
	//connection
	public void adminLoginAccount() {
		GetDetails getdetails = new GetDetails();
		getdetails.getAdminLoginDetails();
		checkAdminLogin checkadminlogin = new checkAdminLogin();
		checkadminlogin.(getdetails.adminUserName,getdetails.adminPassWord);
	}
}
